            </Grid.Background>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <TextBlock  x:Name="_winnerMessageTextBlock" Text="Rickben Gimeda Win this round!" FontSize="120" Panel.ZIndex="2"
                        TextAlignment="Center" TextWrapping="Wrap" Grid.Row="0" Grid.RowSpan="4"
                        Grid.Column="1" Grid.ColumnSpan="3" FontWeight="Bold" FontStyle="Italic" FontFamily="Jokerman" Foreground="#FF4CEE00"/>
